
@{
    ViewBag.Title = "PreviewCryptoDecrypt";
}

@if (!string.IsNullOrEmpty(ViewBag.Location))
{

    <div class="jumbotron" id="enc">

        <div class="encrypt-header">
            <h3>Текст було розшифровано</h3>
        </div>
        <a href="@Url.Action("DownloadCryptoDecrypt", new { path = ViewBag.Location })" class="action-btn">Зaвантажити файл</a><br />
    </div>
}

